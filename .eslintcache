 = (newRating) => {\r\n        setRating(\r\n            newRating\r\n        )\r\n    }\r\n    // Search Function\r\n    const [searchValue, setSearchValue] = useState('');\r\n    useEffect(() => {\r\n        var newSearchedMovies = props.list.filter(result => result.title.toUpperCase().includes(searchValue.toUpperCase()) && result.rate >= rating)\r\n        props.setMovies(newSearchedMovies)\r\n    },[searchValue, rating])\r\n\r\n    return (\r\n        <div className='row heading'> \r\n            <h1>{props.heading}</h1>\r\n            <SearchBox\r\n                searchValue={searchValue}\r\n                setSearchValue={setSearchValue} />\r\n            <StarRatings\r\n                rating={rating}\r\n                starRatedColor=\"yellow\"\r\n                starHoverColor='yellow'\r\n                changeRating={changeRating}\r\n                numberOfStars={5}\r\n                starDimension='40px'\r\n                starSpacing='2px'\r\n                name='rating'\r\n            />\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default MovieListHeading;","C:\\Users\\Mahmoud\\OneDrive\\Desktop\\dev\\myflix\\src\\components\\SearchBox.jsx",[],"C:\\Users\\Mahmoud\\OneDrive\\Desktop\\dev\\myflix\\src\\components\\AddMovie.jsx",[],{"ruleId":"36","severity":1,"message":"37","line":5,"column":8,"nodeType":"38","messageId":"39","endLine":5,"endColumn":16},{"ruleId":"36","severity":1,"message":"37","line":7,"column":8,"nodeType":"38","messageId":"39","endLine":7,"endColumn":16},{"ruleId":"40","severity":1,"message":"41","line":17,"column":7,"nodeType":"42","endLine":17,"endColumn":28,"suggestions":"43"},"no-unused-vars","'AddMovie' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'props'. Either include it or remove the dependency array. However, 'props' will change when *any* prop changes, so the preferred fix is to destructure the 'props' object outside of the useEffect